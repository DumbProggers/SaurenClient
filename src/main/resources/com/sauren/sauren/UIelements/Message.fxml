<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="209.0" prefWidth="425.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sauren.sauren.UIelements.MessageController">
   <children>
      <VBox layoutY="-6.0" prefHeight="224.0" prefWidth="425.0">
         <children>
            <Label fx:id="message" alignment="CENTER" prefHeight="188.0" prefWidth="425.0" text="Label">
               <font>
                  <Font name="Comic Sans MS" size="23.0" />
               </font>
            </Label>
            <Button fx:id="button" mnemonicParsing="false" onAction="#setActionButton" prefHeight="44.0" prefWidth="425.0" text="OK">
               <font>
                  <Font name="Comic Sans MS" size="22.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
